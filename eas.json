{
  "cli": {
    "version": ">= 11.0.3"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "extends": "base",
      "env": {
        "NODE_ENV": "development"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "extends": "base",
      "channel": "preview"
    },
    "production": {
      "extends": "base",
      "channel": "production"
    },
    "base": {
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_FAST_RESOLVER": "1"
      },
      "channel": "base"
    }
  },
  "submit": {
    "production": {}
  }
}
